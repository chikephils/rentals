{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/v2/(.*)",
            "dest": "https://your-backend.vercel.app/api/v2/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}